{deps, [{esockd, {git, "https://gitee.com/fastdgiot/esockd", {tag, "5.8.0"}}}]}.
       , {gen_coap, {git, "https://gitee.com/fastdgiot/gen_coap", {tag, "v0.4.2"}}}

{dialyzer, [
  {warnings, [unmatched_returns, error_handling, race_conditions]},
  {plt_prefix, ".coap_dialyzer"},
